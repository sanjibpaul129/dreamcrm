<h1>Sales EM</h1><br>
<%= form_tag 'sales_em', :html => {:class => 'form-horizontal'} do %>  
	<div class="row">
		<div class="col-lg-1">
			From:
		</div>
		<div class="col-lg-2">
			<%= date_field_tag 'from', @from.to_date, :class => 'form-control' %>
		</div>
		<div class="col-lg-1">
			To:
		</div>
		<div class="col-lg-2">
			<%= date_field_tag 'to', @to.to_date, :class => 'form-control' %>
		</div>
		<div class="col-lg-2">
			<%= submit_tag 'View Details', :class => 'btn btn-primary' %>
		</div>
	</div>	
<%end%>
<table class="customers">
	<thead>
		<th>Project</th>
		<th>Name</th>
		<th>Role</th>
		<th>Online Fresh</th>
		<th>Qualify</th>
		<th>Offline Fresh</th>
		<th>Qualify</th>
		<th>Online Qualified</th>
		<th>Site Visited</th>
		<th>Offline Qualified</th>
		<th>Site Visited</th>
	</thead>
	<tbody>
		<% @gurukul_data.each do |key, value| %>
			<tr>
				<td>Dream Gurukul</td>
				<td><%= key%></td>
				<%value.each do |data|%>
					<td><%= data%></td>
				<%end%>
			</tr>
		<%end%>
	</tbody>
</table>